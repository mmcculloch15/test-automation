{
  "name": "todos-app",
  "version": "1.0.0",
  "description": "Sample Todos application to be used to explain test automation frameworks",
  "main": "server.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "serve": "node .",
    "dev": "concurrently \"npm run watch\" \"npm run serve\"",
    "start": "yarn build && yarn serve",
    "test:wdio": "./node_modules/.bin/wdio ./webdriverio/wdio.conf.js",
    "test:cy": "./node_modules/.bin/cypress open",
    "test:cy:headless": "./node_modules/.bin/cypress run --spec 'cypress/integration/todosApp/*' ",
    "cy:verify": "./node_modules/.bin/cypress verify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.17.0",
    "concurrently": "^3.5.0",
    "cypress": "^3.4.1",
    "json-server": "^0.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^4.2.2",
    "webdriverio": "^5.12.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@wdio/browserstack-service": "^5.12.1",
    "@wdio/cli": "^5.12.4",
    "@wdio/local-runner": "^5.12.4",
    "@wdio/mocha-framework": "^5.12.1",
    "@wdio/selenium-standalone-service": "^5.12.1",
    "@wdio/spec-reporter": "^5.12.1",
    "@wdio/sync": "^5.12.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^76.0.0",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "start-server-and-test": "^1.10.0",
    "style-loader": "^0.19.0",
    "wdio-chromedriver-service": "^5.0.2",
    "webpack": "^3.6.0"
  }
}
