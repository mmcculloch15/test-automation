{
	"name": "todos-app",
	"version": "1.0.0",
	"description": "Sample Todos application to be used to explain test automation frameworks",
	"main": "server.js",
	"scripts": {
		"build": "webpack",
		"watch": "webpack --watch",
		"serve": "node .",
		"dev": "concurrently \"npm run watch\" \"npm run serve\"",
		"start": "yarn build && yarn serve",
		"test:wdio": "./node_modules/.bin/wdio ./webdriverio/wdio.conf.js",
		"test:cy": "./node_modules/.bin/cypress open",
		"test:cy:headless": "./node_modules/.bin/cypress run --spec 'cypress/integration/todosApp/*' ",
		"cy:verify": "./node_modules/.bin/cypress verify",
		"merge-reports": "./node_modules/.bin/mochawesome-merge --reportDir ./results > mochawesome.json",
		"generate-report": "./node_modules/.bin/marge ./mochawesome.json"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/plugin-proposal-object-rest-spread": "^7.6.2",
		"@babel/preset-react": "^7.0.0",
		"@wdio/junit-reporter": "^5.13.2",
		"axios": "^0.17.0",
		"concurrently": "^3.5.0",
		"cypress": "^3.4.1",
		"cypress-multi-reporters": "^1.2.1",
		"dotenv": "^8.1.0",
		"json-server": "^0.12.0",
		"mocha": "^5.2.0",
		"mochawesome": "^3.1.2",
		"mochawesome-merge": "^1.0.7",
		"mochawesome-report-generator": "^3.1.5",
		"react": "^16.10.1",
		"react-dom": "^16.10.1",
		"react-router-dom": "^4.2.2",
		"wdio-mochawesome-reporter": "^3.2.0",
		"webdriverio": "^5.14.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.2",
		"@babel/core": "^7.6.2",
		"@babel/preset-env": "^7.6.2",
		"@babel/register": "^7.6.2",
		"@wdio/browserstack-service": "^5.13.2",
		"@wdio/cli": "^5.14.3",
		"@wdio/local-runner": "^5.14.3",
		"@wdio/mocha-framework": "^5.14.3",
		"@wdio/selenium-standalone-service": "^5.13.2",
		"@wdio/spec-reporter": "^5.13.2",
		"@wdio/sync": "^5.14.3",
		"babel-loader": "^8.0.6",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.0",
		"babel-preset-react": "^6.24.1",
		"chromedriver": "^76.0.1",
		"css-loader": "^0.28.7",
		"html-webpack-plugin": "^2.30.1",
		"start-server-and-test": "^1.10.3",
		"style-loader": "^0.19.0",
		"wdio-chromedriver-service": "^5.0.2",
		"webpack": "^3.6.0"
	}
}
